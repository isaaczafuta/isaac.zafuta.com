  "scripts": {
    "start": "./node_modules/.bin/npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "build-css": "./node_modules/.bin/node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "yarn run build-css && ./node_modules/.bin/node-sass-chokidar --include-path ./src/ --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build-js": "./node_modules/.bin/react-scripts build",
    "build": "./node_modules/.bin/npm-run-all build-css build-js",
    "test": "./node_modules/.bin/react-scripts test --env=jsdom",
    "eject": "./node_modules/.bin/react-scripts eject"
