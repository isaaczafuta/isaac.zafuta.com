{
  "name": "Node.js & TypeScript",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "14"
    }
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": ["esbenp.prettier-vscode"],
  "postCreateCommand": "npm ci",
  "remoteUser": "node",
  "portsAttributes": {
    "3000": {
      "label": "Web"
    }
  }
}
